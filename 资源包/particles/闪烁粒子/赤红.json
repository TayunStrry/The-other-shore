{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "闪烁粒子:赤红",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "textures/粒子贴图/基础纹理"
			}
		},
		"components": {
			"minecraft:emitter_rate_instant": {
				"num_particles": 1
			},
			"minecraft:emitter_lifetime_once": {},
			"minecraft:particle_initial_speed": 0.0,
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "Math.Random(0.75, 0.9)"
			},
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": [
					0.1,
					0.1,
					0.1
				],
				"linear_drag_coefficient": 0.49
			},
			"minecraft:particle_appearance_billboard": {
				"size": [
					"(variable.particle_random_1 * 0.5f + 0.5f) * 0.2",
					"(variable.particle_random_1 * 0.5f + 0.5f) * 0.2"
				],
				"facing_camera_mode": "lookat_xyz",
				"uv": {
					"texture_width": 128,
					"texture_height": 128,
					"flipbook": {
						"base_UV": [
							64,
							0
						],
						"size_UV": [
							8,
							8
						],
						"step_UV": [
							-8,
							0
						],
						"frames_per_second": 8,
						"max_frame": 8,
						"stretch_to_lifetime": true,
						"loop": false
					}
				}
			},
			"minecraft:particle_appearance_tinting": {
				"color": [
					"variable.particle_age > (variable.particle_lifetime / Math.random(2.0, 1.0)) ? 0.50 : 1.00",
					0.0,
					0.0,
					"variable.particle_age > (variable.particle_lifetime / Math.random(2.0, 1.0)) ? 0.75 : 1.00"
				]
			},
			"minecraft:particle_appearance_lighting": {}
		}
	}
}