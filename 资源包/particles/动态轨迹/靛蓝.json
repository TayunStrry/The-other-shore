{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "动态轨迹:靛蓝",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "textures/粒子贴图/基础纹理"
			}
		},
		"components": {
			"minecraft:emitter_rate_instant": {
				"num_particles": 1
			},
			"minecraft:emitter_lifetime_once": {},
			"minecraft:particle_initial_speed": 0.0,
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "Math.Random(3.0, 5.0)"
			},
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": [
					"Math.random(-0.02 , 0.02)",
					"Math.random(-0.02 , 0.02)",
					"Math.random(-0.02 , 0.02)"
				]
			},
			"minecraft:particle_appearance_billboard": {
				"size": [
					0.1,
					0.1
				],
				"facing_camera_mode": "lookat_xyz",
				"uv": {
					"texture_width": 128,
					"texture_height": 128,
					"flipbook": {
						"base_UV": [
							56,
							50
						],
						"size_UV": [
							8,
							8
						],
						"step_UV": [
							-8,
							0
						],
						"stretch_to_lifetime": true,
						"frames_per_second": 1,
						"max_frame": 3,
						"loop": true
					}
				}
			},
			"minecraft:particle_appearance_tinting": {
				"color": [
					"variable.particle_age > (variable.particle_lifetime / Math.random(6.0, 0.0)) ? 0.0 : 0.1",
                    "variable.particle_age > (variable.particle_lifetime / Math.random(6.0, 0.0)) ? 0.5 : 1.0",
                    "variable.particle_age > (variable.particle_lifetime / Math.random(6.0, 0.0)) ? 0.5 : 1.0",
                    "variable.particle_age > (variable.particle_lifetime / Math.random(6.0, 0.0)) ? 1.0 : 1.0"
				]
			},
			"minecraft:particle_appearance_lighting": {}
		}
	}
}