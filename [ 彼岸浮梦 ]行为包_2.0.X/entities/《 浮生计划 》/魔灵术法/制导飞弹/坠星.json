{
    "format_version": "1.19.10",
    "minecraft:entity": {
        "description": {
            "identifier": "制导飞弹:坠星",
            "is_spawnable": false,
            "is_summonable": true,
            "is_experimental": false
        },
        "component_groups": {
            "书页": {
                "minecraft:behavior.random_hover": {
                    "y_offset": -1,
                    "interval": 1,
                    "priority": 5,
                    "xz_dist": 5,
                    "y_dist": 5,
                    "hover_height": [
                        0,
                        1
                    ]
                },
                "minecraft:behavior.melee_attack": {
                    "priority": 0,
                    "attack_once": false,
                    "track_target": true,
                    "speed_multiplier": 1.4,
                    "on_attack": {
                        "event": "事件:攻击命中",
                        "target": "self"
                    }
                },
                "minecraft:underwater_movement": {
                    "value": 0.1
                },
                "minecraft:navigation.hover": {
                    "can_path_from_air": true,
                    "is_amphibious": true,
                    "can_swim": true,
                    "can_sink": true
                },
                "minecraft:behavior.float": {
                    "priority": 10
                },
                "minecraft:flying_speed": {
                    "value": 0.3
                },
                "minecraft:scale": {
                    "value": 0.5
                },
                "minecraft:ageable": {
                    "duration": 10,
                    "grow_up": {
                        "event": "事件:实体消失",
                        "target": "self"
                    }
                },
                "minecraft:attack": {
                    "damage": {
                        "range_min": 3,
                        "range_max": 5
                    }
                },
                "minecraft:can_fly": {},
                "minecraft:movement.hover": {}
            },
            "符文": {
                "minecraft:navigation.generic": {
                    "can_path_over_water": true,
                    "is_amphibious": true,
                    "can_swim": true,
                    "can_walk": true
                },
                "minecraft:movement": {
                    "value": 0.0
                },
                "minecraft:variant": {
                    "value": 1
                },
                "minecraft:ageable": {
                    "duration": 5,
                    "grow_up": {
                        "event": "事件:实体消失",
                        "target": "self"
                    }
                },
                "minecraft:timer": {
                    "time": 0.01,
                    "time_down_event": {
                        "event": "事件:符文牵引",
                        "target": "self"
                    }
                },
                "minecraft:scale": {
                    "value": 1.0
                },
                "minecraft:movement.hover": {}
            },
            "消失": {
                "minecraft:instant_despawn": {}
            }
        },
        "components": {
            "minecraft:behavior.nearest_attackable_target": {
                "priority": 0,
                "entity_types": [
                    {
                        "filters": {
                            "all_of": [
                                {
                                    "test": "is_family",
                                    "subject": "other",
                                    "value": "monster"
                                }
                            ]
                        },
                        "within_default": 64
                    }
                ],
                "must_reach": true,
                "must_see": true
            },
            "minecraft:collision_box": {
                "width": 0.1,
                "height": 0.1
            },
            "minecraft:type_family": {
                "family": [
                    "spirit",
					"unreal",
                    "prime"
                ]
            },
            "minecraft:breathable": {
                "totalSupply": 10,
                "suffocateTime": 0,
                "breathes_air": true,
                "breathes_lava": true,
                "breathes_water": true,
                "breathes_solids": true
            },
            "minecraft:pushable": {
                "is_pushable": false,
                "is_pushable_by_piston": false
            },
            "minecraft:physics": {
                "has_gravity": true,
                "has_collision": true
            },
            "minecraft:health": {
                "value": 10
            },
            "minecraft:fire_immune": true
        },
        "events": {
            "minecraft:entity_spawned": {
                "add": {
                    "component_groups": [
                        "书页"
                    ]
                }
            },
            "事件:实体消失": {
                "add": {
                    "component_groups": [
                        "消失"
                    ]
                }
            },
            "事件:星芒爆发": {
                "add": {
                    "component_groups": [
                        "消失"
                    ]
                }
            },
            "事件:攻击命中": {
                "add": {
                    "component_groups": [
                        "符文"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "书页"
                    ]
                }
            },
            "事件:符文牵引": {
                "run_command": {
                    "command": [
                        "execute as @s [tag=!RuneHit] run spreadplayers ~ ~ 0 1 @e[r=8,family=monster,c=1]",
                        "execute as @s [tag=!RuneHit] run spreadplayers ~ ~ 0 1 @s",
                        "tp @e[r=8,family=monster] ^ ^0.05 ^0.5 facing @s",
                        "tp @e[r=16,rm=0.1,tag=RuneHit] @s",
                        "tag @s add RuneHit"
                    ]
                }
            }
        }
    }
}