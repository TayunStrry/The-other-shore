            "事件:载具开关": {
                "sequence": [
                    { //关闭 载具运行
                        "filters": {
                            "all_of": [
                                {
                                    "test": "has_tag",
                                    "operator": "==",
                                    "value": "tunnel_train.function_open"
                                }
                            ]
                        },
                        "run_command": {
                            "command": [
                                "tag @s remove tunnel_train.function_open"
                            ]
                        }
                    },
                    { //开启 载具运行
                        "filters": {
                            "all_of": [
                                {
                                    "test": "has_tag",
                                    "operator": "!=",
                                    "value": "tunnel_train.function_open"
