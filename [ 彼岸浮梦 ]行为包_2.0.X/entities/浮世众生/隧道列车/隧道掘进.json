            "事件:载具功能": {
                "sequence": [
                    {
                        "filters": {
                            "all_of": [
                                {
                                    "test": "has_tag",
                                    "operator": "==",
                                    "value": "tunnel_train.function_open"
                                }
                            ]
                        },
                        "sequence": [
                            { // 执行 挖掘方块 照亮环境 消耗能量 铺设方块 的任务
                                "filters": {
                                    "all_of": [
                                        {
                                            "test": "has_tag",
                                            "operator": "!=",
                                            "value": "tunnel_train.Dextral"
                                        },
                                        {
                                            "test": "has_tag",
                                            "operator": "!=",
                                            "value": "tunnel_train.Sinistral"
