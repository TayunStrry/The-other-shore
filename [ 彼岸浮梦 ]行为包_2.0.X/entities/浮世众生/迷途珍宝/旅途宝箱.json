                    },
                    {
                        "filters": {
                            "all_of": [
                                {
                                    "test": "has_tag",
                                    "operator": "==",
                                    "value": "RareBox_0"
                                },
                                {
                                    "test": "has_tag",
                                    "operator": "==",
                                    "value": "RareBox_1"
                                },
                                {
                                    "test": "has_tag",
                                    "operator": "!=",
                                    "value": "RareBox_2"
                                }
                            ]
                        },
                        "randomize": [
                            {
                                "weight": 1,
                                "run_command": {
                                    "command": [
                                        "loot spawn ~~1~ loot 珍宝箱_容器"
                                    ]
                                }
                            },
                            {
                                "weight": 15,
                                "run_command": {
                                    "command": [
                                        "loot spawn ~~1~ loot 珍宝箱_魔书",
                                        "tag @s add RareBox_2"
                                    ]
                                }
                            }
                        ]
                    },
                    {
                        "filters": {
                            "all_of": [
                                {
                                    "test": "has_tag",
                                    "operator": "==",
                                    "value": "RareBox_0"
                                },
                                {
                                    "test": "has_tag",
                                    "operator": "==",
                                    "value": "RareBox_1"
                                },
                                {
                                    "test": "has_tag",
                                    "operator": "==",
                                    "value": "RareBox_2"
                                }
                            ]
                        },
                        "run_command": {
                            "command": [
                                "loot spawn ~~1~ loot 珍宝箱_杂物"
                            ]
                        }
                    }
                ]
            },
            "事件:发现目标": {
                "run_command": {
                    "command": [
                        "execute as @s [tag=!call] run summon 天渊阁:茉莉 ~-0.5 ~ ~-0.5",
                        "execute as @s [tag=!call] run summon 天渊阁:琥珀 ~0.5 ~ ~0.5",
                        "tag @s add call"
                    ]
                }
            },
            "事件:实体消失": {
                "add": {
                    "component_groups": [
                        "消失"
                    ]
                }
            }
        }
