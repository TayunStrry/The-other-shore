                    {
                        "filters": {
                            "all_of": [
                                {
                                    "test": "has_tag",
                                    "operator": "==",
                                    "value": "RareBox_0"
                                },
                                {
                                    "test": "has_tag",
                                    "operator": "!=",
                                    "value": "RareBox_1"
                                }
                            ]
                        },
                        "run_command": {
                            "command": [
                                "loot spawn ~~1~ loot 珍宝箱_矿物",
                                "tag @s add RareBox_1"
