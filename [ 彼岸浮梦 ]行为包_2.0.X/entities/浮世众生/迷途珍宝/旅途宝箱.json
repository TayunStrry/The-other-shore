{
    "format_version": "1.19.10",
    "minecraft:entity": {
        "description": {
            "identifier": "珍宝箱:旅途",
            "is_experimental": false,
            "is_summonable": true,
            "is_spawnable": true
        },
        "component_groups": {
            "交互": {
                "minecraft:variant": {
                    "value": 0
                },
                "minecraft:timer": {
                    "time": 1.0,
                    "time_down_event": {
                        "event": "事件:文本提示",
                        "target": "self"
                    }
                },
                "minecraft:interact": {
                    "interactions": [
                        {
                            "cooldown": 1,
                            "use_item": true,
                            "interact_text": "开启 <迷途珍宝>",
                            "play_sounds": "armor.equip_leather",
                            "on_interact": {
                                "filters": {
                                    "all_of": [
                                        {
                                            "test": "is_family",
                                            "subject": "other",
                                            "value": "player"
                                        },
                                        {
                                            "test": "has_equipment",
                                            "domain": "hand",
                                            "subject": "other",
                                            "value": "魔法工具:魔晶钥匙"
                                        }
                                    ]
                                },
                                "event": "事件:开启宝箱",
                                "target": "self"
                            }
                        }
                    ]
                },
                "minecraft:despawn": {
                    "filters": {
                        "all_of": [
                            {
                                "test": "distance_to_nearest_player",
                                "operator": ">",
                                "value": 64
                            }
                        ]
                    }
                }
            },
            "释放": {
                "minecraft:variant": {
                    "value": 1
                },
                "minecraft:ageable": {
                    "duration": 5,
                    "grow_up": {
                        "event": "事件:实体消失",
                        "target": "self"
                    }
                },
                "minecraft:timer": {
                    "time": 1.0,
                    "time_down_event": {
                        "event": "事件:释放物资",
                        "target": "self"
                    }
                }
            },
            "悬浮": {
                "minecraft:movement.hover": {},
                "minecraft:flying_speed": {
                    "value": 0.0
                },
                "minecraft:behavior.hurt_by_target": {
                    "priority": 1
                },
                "minecraft:on_target_acquired": {
                    "event": "事件:发现目标",
                    "target": "self"
                },
                "minecraft:navigation.hover": {
                    "can_path_from_air": true,
                    "is_amphibious": true,
                    "can_swim": true,
                    "can_sink": true
                }
            },
            "消失": {
                "minecraft:instant_despawn": {}
            }
        },
        "components": {
            "minecraft:conditional_bandwidth_optimization": {},
            "minecraft:is_stackable": {},
            "minecraft:type_family": {
                "family": [
                    "RareBox",
                    "make"
                ]
            },
            "minecraft:health": {
                "value": 999
            },
            "minecraft:scale": {
                "value": 0.75
            },
            "minecraft:fire_immune": true,
            "minecraft:damage_sensor": {
                "triggers": [
                    {
                        "cause": "fall",
                        "deals_damage": false
                    },
                    {
                        "cause": "projectile",
                        "deals_damage": true
                    }
                ]
            },
            "minecraft:knockback_resistance": {
                "value": 1.0
            },
            "minecraft:collision_box": {
                "width": 0.3,
                "height": 0.6
            },
            "minecraft:physics": {},
            "minecraft:pushable": {
                "is_pushable": false,
                "is_pushable_by_piston": true
            }
        },
        "events": {
            "minecraft:entity_spawned": {
                "sequence": [
                    {
                        "randomize": [
                            {
                                "weight": 1,
                                "add": {
                                    "component_groups": [
                                        "交互",
                                        "悬浮"
                                    ]
                                },
                                "run_command": {
                                    "command": [
                                        "execute as @p[tag=!ambient] run function Data/ambient"
                                    ]
                                }
                            },
                            {
                                "weight": 4,
                                "add": {
                                    "component_groups": [
                                        "消失"
                                    ]
                                }
                            }
                        ]
                    }
                ]
            },
            "事件:文本提示": {
                "run_command": {
                    "command": [
                        "title @p[r=2] actionbar 使用 魔晶钥匙 开启宝箱"
