				"run_command": {
					"command": [
						"execute as @p[tag=!ambient] run function Data/ambient",
						"effect @s resistance 5 255 false",
						"summon 增幅法阵:星渊 ~~~",
						"summon 增幅法阵:星渊 ~~~",
						"summon 增幅法阵:星渊 ~~~"
					]
				}
			},
			"发现目标": {
