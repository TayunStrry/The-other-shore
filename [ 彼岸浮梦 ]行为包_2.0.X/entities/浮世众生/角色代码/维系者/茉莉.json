								},
								{
									"test": "is_family",
									"subject": "other",
									"value": "Tayun"
								},
								{
									"test": "is_family",
									"subject": "other",
									"value": "warden"
								},
								{
									"all_of": [
										{
											"test": "is_family",
											"operator": "==",
											"subject": "other",
											"value": "monster"
										},
										{
											"test": "is_family",
											"operator": "!=",
											"subject": "other",
											"value": "abyss"
										}
									]
								}
							]
						},
						"within_default": 16
					}
				],
