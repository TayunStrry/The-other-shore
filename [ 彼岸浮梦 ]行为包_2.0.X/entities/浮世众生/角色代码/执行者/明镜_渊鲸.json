{
    "format_version": "1.19.10",
    "minecraft:entity": {
        "description": {
            "identifier": "明镜阁:渊鲸",
            "is_spawnable": true,
            "is_summonable": true,
            "is_experimental": false,
            "scripts": {
                "animate": [
                    "death"
                ]
            },
            "animations": {
                "death": "controller.animation.data_death"
            }
        },
        "component_groups": {
            "自由": {
                "minecraft:behavior.nearest_attackable_target": {
                    "priority": 4,
                    "entity_types": [
                        {
                            "filters": {
                                "all_of": [
                                    {
                                        "test": "is_family",
                                        "subject": "other",
                                        "operator": "==",
                                        "value": "monster"
                                    }
                                ]
                            },
                            "within_default": 32
                        }
                    ],
                    "must_reach": true,
                    "must_see": true
                },
                "minecraft:damage_sensor": {
                    "triggers": [
                        {
                            "cause": "*",
                            "on_damage": {
                                "target": "self",
                                "filters": {
                                    "any_of": [
                                        {
                                            "test": "is_family",
                                            "subject": "other",
                                            "value": "player"
                                        }
                                    ]
                                }
                            },
                            "deals_damage": false
                        },
                        {
                            "cause": "fall",
                            "deals_damage": false
                        },
                        {
                            "cause": "projectile",
                            "deals_damage": false
                        }
                    ]
                },
                "minecraft:behavior.random_swim": {
                    "priority": 3,
                    "speed_multiplier": 1.0,
                    "xz_dist": 16,
                    "y_dist": 4,
                    "interval": 0
                },
                "minecraft:behavior.swim_wander": {
                    "priority": 4,
                    "interval": 0.1,
                    "look_ahead": 2.0,
                    "speed_multiplier": 1.0,
                    "wander_time": 5.0
                },
                "minecraft:underwater_movement": {
                    "value": 0.15
                },
                "minecraft:behavior.swim_idle": {
                    "priority": 5,
                    "idle_time": 5.0,
                    "success_rate": 0.1
                },
                "minecraft:navigation.generic": {
                    "can_path_over_water": false,
                    "is_amphibious": false,
                    "can_breach": false,
                    "can_walk": false,
                    "can_sink": false,
                    "can_swim": true
                },
                "minecraft:experience_reward": {
                    "on_death": "query.last_hit_by_player ? 15 : 1"
                },
                "minecraft:movement.sway": {
                    "sway_amplitude": 0.0
                },
                "minecraft:movement": {
                    "value": 0.15
                },
                "minecraft:health": {
                    "value": 100
                },
                "minecraft:tameable": {
                    "probability": 1.0,
                    "tame_items": [
                        "魔法书籍:精灵结契",
                        "minecraft:pumpkin_pie"
                    ],
                    "tame_event": {
                        "event": "事件:缔结契约",
                        "target": "self"
                    }
                },
                "minecraft:despawn": {
                    "filters": {
                        "all_of": [
                            {
                                "test": "distance_to_nearest_player",
                                "operator": ">",
                                "value": 48
                            }
                        ]
                    }
                },
                "minecraft:loot": {
                    "table": "loot_tables/无人机.json"
                }
            },
            "契约": {
                "minecraft:behavior.controlled_by_player": {
                    "priority": 0
                },
                "minecraft:behavior.owner_hurt_by_target": {
                    "priority": 4
                },
                "minecraft:behavior.owner_hurt_target": {
                    "priority": 4
                },
                "minecraft:knockback_resistance": {
                    "value": 1.0
                },
                "minecraft:underwater_movement": {
                    "value": 0.15
                },
                "minecraft:navigation.generic": {
                    "can_path_over_water": true,
                    "avoid_damage_blocks": true,
                    "can_pass_doors": true,
                    "can_open_doors": false,
                    "is_amphibious": true,
                    "can_swim": true,
                    "can_walk": true
                },
                "minecraft:health": {
                    "value": 20
                },
                "minecraft:damage_sensor": {
                    "triggers": [
                        {
                            "cause": "*",
                            "on_damage": {
                                "event": "事件:回收载具",
                                "target": "self"
                            },
                            "deals_damage": false
                        }
                    ]
                },
                "minecraft:leashable": {
                    "soft_distance": 5.0,
                    "max_distance": 15.0,
                    "hard_distance": 10.0,
                    "on_leash": {
                        "event": "minecraft:on_leash",
                        "target": "self"
                    },
                    "on_unleash": {
                        "event": "minecraft:on_unleash",
                        "target": "self"
                    }
                },
                "minecraft:scheduler": {
                    "min_delay_secs": 4,
                    "max_delay_secs": 8,
                    "scheduled_events": [
                        {
                            "filters": {
                                "all_of": [
                                    {
                                        "test": "is_underwater",
                                        "operator": "==",
                                        "value": true
                                    }
                                ]
                            },
                            "event": "事件:开启供氧",
                            "target": "self"
                        },
                        {
                            "filters": {
                                "all_of": [
                                    {
                                        "test": "is_underwater",
                                        "operator": "==",
                                        "value": false
                                    },
                                    {
                                        "test": "on_ground",
                                        "operator": "==",
                                        "value": false
                                    }
                                ]
                            },
                            "event": "事件:关闭供氧",
                            "target": "self"
                        },
                        {
                            "filters": {
                                "all_of": [
                                    {
                                        "test": "is_underwater",
                                        "operator": "==",
                                        "value": false
                                    },
                                    {
                                        "test": "on_ground",
                                        "operator": "==",
                                        "value": true
                                    }
                                ]
                            },
                            "event": "事件:船只搁浅",
                            "target": "self"
                        }
                    ]
                },
                "minecraft:movement": {
                    "value": 0.15
                },
                "minecraft:rideable": {
                    "seat_count": 1,
                    "family_types": [
                        "player"
                    ],
                    "pull_in_entities": true,
                    "seats": [
                        {
                            "position": [
                                0.0,
                                0.4,
                                0.0
                            ],
                            "min_rider_count": 0,
                            "max_rider_count": 1
                        }
                    ]
                },
                "minecraft:interact": {
                    "interactions": [
                        {
                            "cooldown": 1,
                            "use_item": false,
                            "interact_text": "组成舰队",
                            "play_sounds": "armor.equip_diamond",
                            "on_interact": {
                                "filters": {
                                    "all_of": [
                                        {
                                            "test": "has_equipment",
                                            "domain": "hand",
                                            "subject": "other",
                                            "value": "特殊道具:召唤渊鲸"
                                        }
                                    ]
                                },
                                "event": "事件:组成舰队",
                                "target": "self"
                            }
                        }
                    ]
                },
                "minecraft:input_ground_controlled": {},
                "minecraft:movement.basic": {},
                "minecraft:is_stackable": {},
                "minecraft:jump.static": {},
                "minecraft:is_tamed": {}
            },
            "上浮": {
                "minecraft:buoyant": {
                    "base_buoyancy": 0.1,
                    "apply_gravity": false,
                    "simulate_waves": true,
                    "liquid_blocks": [
                        "minecraft:water",
                        "minecraft:flowing_water"
                    ]
                }
            },
            "战斗": {
                "minecraft:behavior.ranged_attack": {
                    "attack_interval_min": 2,
                    "attack_interval_max": 4,
                    "attack_radius": 32
                },
                "minecraft:angry": {
                    "duration": 8,
                    "broadcast_anger": true,
                    "broadcast_range": 16,
                    "broadcast_targets": [
                        "ocean"
                    ],
                    "broadcast_filters": {
                        "test": "is_visible",
                        "subject": "other",
                        "value": true
                    },
                    "calm_event": {
                        "event": "事件:放弃进攻",
                        "target": "self"
                    }
                },
                "minecraft:shooter": {
                    "def": "弹道飞弹:星陨"
                }
            },
            "氧气": {
                "minecraft:timer": {
                    "time": 5.0,
                    "time_down_event": {
                        "event": "事件:氧气提供",
                        "target": "self"
                    }
                }
            },
            "舰队": {
                "minecraft:spawn_entity": {
                    "entities": [
                        {
                            "spawn_event": "事件:跟随旗舰",
                            "spawn_entity": "明镜阁:渊鲸",
                            "should_leash": true,
                            "single_use": true,
                            "min_wait_time": 0,
                            "max_wait_time": 0,
                            "num_to_spawn": 1
                        }
                    ]
                }
            },
            "航行": {
                "minecraft:movement": {
                    "value": 0.15
                },
                "minecraft:underwater_movement": {
                    "value": 0.15
                }
            },
            "禁航": {
                "minecraft:movement": {
                    "value": 0.0
                },
                "minecraft:underwater_movement": {
                    "value": 0.0
                }
            },
            "消失": {
                "minecraft:instant_despawn": {}
            }
        },
        "components": {
            "minecraft:on_target_acquired": {
                "event": "事件:发现目标",
                "target": "self"
            },
            "minecraft:conditional_bandwidth_optimization": {
                "conditional_values": {
                    "max_dropped_ticks": 20,
                    "max_optimized_distance": 200,
                    "use_motion_prediction_hints": true
                },
                "default_values": {
                    "max_dropped_ticks": 10,
                    "max_optimized_distance": 100,
                    "use_motion_prediction_hints": true
                }
            },
            "minecraft:scale": {
                "value": 3
            },
            "minecraft:collision_box": {
                "width": 0.65,
                "height": 1.0
            },
            "minecraft:type_family": {
                "family": [
                    "prime",
                    "Tayun",
                    "ocean",
                    "ocean_mount"
                ]
            },
            "minecraft:breathable": {
                "totalSupply": 10,
                "suffocateTime": 0,
                "breathes_air": true,
                "breathes_lava": true,
                "breathes_water": true,
                "breathes_solids": false
            },
            "minecraft:pushable": {
                "is_pushable": true,
                "is_pushable_by_piston": true
            },
            "minecraft:physics": {
                "has_gravity": true
            },
            "minecraft:fire_immune": true
        },
        "events": {
            "minecraft:entity_spawned": {
                "run_command": {
                    "command": [
                        "execute as @p[tag=!ambient] run function Data/ambient"
                    ]
                },
                "add": {
                    "component_groups": [
                        "自由"
                    ]
                }
            },
            "事件:发现目标": {
                "sequence": [
                    {
                        "run_command": {
                            "command": [
                                "execute as @s [tag=is_tamed] run title @a[r=32] actionbar 【 @s 】: 目标已锁定 正在引导舰炮攻击......"
                            ]
                        }
                    },
                    {
                        "add": {
                            "component_groups": [
                                "战斗"
                            ]
                        }
                    }
                ]
            },
            "事件:放弃进攻": {
                "sequence": [
                    {
                        "run_command": {
                            "command": [
                                "execute as @s [tag=is_tamed] run title @a[r=32] actionbar 【 @s 】: 目标已丢失 正在重新寻找目标......"
                            ]
                        }
                    },
                    {
                        "remove": {
                            "component_groups": [
                                "战斗"
                            ]
                        }
                    }
                ]
            },
            "事件:上浮模式": {
                "sequence": [
                    {
                        "run_command": {
                            "command": [
                                "execute as @s [tag=is_tamed] run title @p[r=5] actionbar 【 @s 】: 正在进入< 上浮模式 >"
                            ]
                        }
                    },
                    {
                        "add": {
                            "component_groups": [
                                "上浮"
                            ]
                        }
                    }
                ]
            },
            "事件:下潜模式": {
                "sequence": [
                    {
                        "run_command": {
                            "command": [
                                "execute as @s [tag=is_tamed] run title @p[r=5] actionbar 【 @s 】: 正在进入< 下潜模式 >"
                            ]
                        }
                    },
                    {
                        "remove": {
                            "component_groups": [
                                "上浮"
                            ]
                        }
                    }
                ]
            },
            "事件:开启供氧": {
                "sequence": [
                    {
                        "run_command": {
                            "command": [
                                "execute as @s [tag=is_tamed] run title @p[r=5] actionbar 【 @s 】: 正在开启< 氧气供应 >",
                                "effect @p[r=5] conduit_power 16 0 true"
                            ]
                        }
                    },
                    {
                        "add": {
                            "component_groups": [
                                "氧气"
                            ]
                        }
                    }
                ]
            },
            "事件:关闭供氧": {
                "sequence": [
                    {
                        "run_command": {
                            "command": [
                                "execute as @s [tag=is_tamed] run title @p[r=5] actionbar 【 @s 】: 正在关闭< 氧气供应 >",
                                "effect @p[r=5] conduit_power 0 1 true"
                            ]
                        }
                    },
                    {
                        "remove": {
                            "component_groups": [
                                "氧气"
                            ]
                        }
                    }
                ]
            },
            "事件:氧气提供": {
                "run_command": {
                    "command": [
                        "effect @s instant_health 1 0 true",
                        "effect @p[r=5] conduit_power 16 0 true"
                    ]
                }
            },
            "事件:船只搁浅": {
                "run_command": {
                    "command": [
                        "execute as @s [tag=is_tamed] run title @p[r=5] actionbar 【 @s 】: 船只已经进入[§4 搁浅状态 §r], 请摆脱[§4 搁浅状态 §r] !"
                    ]
                },
                "add": {
                    "component_groups": [
                        "禁航"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "航行"
                    ]
                }
            },
            "事件:解除禁航": {
                "add": {
                    "component_groups": [
                        "航行"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "禁航"
                    ]
                }
            },
            "事件:缔结契约": {
                "run_command": {
                    "command": [
                        "function RoleCode/EnterContracts",
                        "tag @s add is_tamed",
                        "give @p 载具控制:浮力开关"
                    ]
                },
                "add": {
                    "component_groups": [
                        "契约",
                        "上浮"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "自由"
                    ]
                }
            },
            "事件:跟随旗舰": {
                "run_command": {
                    "command": [
                        "tag @s add is_tamed"
                    ]
                },
                "add": {
                    "component_groups": [
                        "契约",
                        "上浮"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "自由"
                    ]
                }
            },
            "事件:组成舰队": {
                "sequence": [
                    {
                        "run_command": {
                            "command": [
                                "particle minecraft:knockback_roar_particle ~~~"
                            ]
                        },
                        "add": {
                            "component_groups": [
                                "舰队"
                            ]
                        }
                    }
                ]
            },
            "事件:回收载具": {
                "sequence": [
                    {
                        "filters": {
                            "any_of": [
                                {
                                    "test": "is_family",
                                    "subject": "other",
                                    "value": "player"
                                }
                            ]
                        },
                        "run_command": {
                            "command": [
                                "particle minecraft:knockback_roar_particle ~~~",
                                "give @p 特殊道具:召唤渊鲸"
                            ]
                        },
                        "add": {
                            "component_groups": [
                                "消失"
                            ]
                        }
                    }
                ]
            },
            "事件:实体消失": {
                "sequence": [
                    {
                        "run_command": {
                            "command": [
                                "particle minecraft:knockback_roar_particle ~~~"
                            ]
                        },
                        "add": {
                            "component_groups": [
                                "消失"
                            ]
                        }
                    }
                ]
            }
        }
    }
}