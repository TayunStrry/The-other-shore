{
    "format_version": "1.19.10",
    "minecraft:entity": {
        "description": {
            "identifier": "弹道飞弹:辉光",
            "is_spawnable": false,
            "is_summonable": true,
            "is_experimental": false
        },
        "component_groups": {
            "消失": {
                "minecraft:instant_despawn": {
                    "remove_child_entities": true
                }
            }
        },
        "components": {
            "minecraft:conditional_bandwidth_optimization": {
                "default_values": {
                    "max_optimized_distance": 80.0,
                    "max_dropped_ticks": 7,
                    "use_motion_prediction_hints": true
                }
            },
            "minecraft:damage_sensor": {
                "triggers": [
                    {
                        "cause": "*",
                        "deals_damage": false
                    }
                ]
            },
            "minecraft:collision_box": {
                "width": 0.1,
                "height": 0.1
            },
            "minecraft:breathable": {
                "totalSupply": 10,
                "suffocateTime": 0,
                "breathes_air": true,
                "breathes_lava": true,
                "breathes_water": true,
                "breathes_solids": true
            },
            "minecraft:projectile": {
                "on_hit": {
                    "impact_damage": {
                        "semi_random_diff_damage": true,
                        "destroy_on_hit": true,
                        "knockback": true,
                        "damage": 0
                    },
                    "definition_event": {
                        "affect_projectile": true,
                        "event_trigger": {
                            "event": "事件:点亮环境",
                            "target": "self"
                        }
                    }
                },
                "hit_sound": "conduit.attack",
                "uncertainty_multiplier": 1,
                "reflect_on_hurt": true,
                "uncertainty_base": 1,
                "should_bounce": false,
                "catch_fire": true,
                "gravity": 0.0,
                "power": 1.6,
                "anchor": 1,
                "offset": [
                    0,
                    0,
                    0
                ]
            },
            "minecraft:physics": {
                "has_gravity": true,
                "has_collision": true
            },
            "minecraft:pushable": {
                "is_pushable": false,
                "is_pushable_by_piston": false
            },
            "minecraft:timer": {
                "time": 0.01,
                "looping": true,
                "time_down_event": {
                    "event": "事件:弹道粒子",
                    "target": "self"
                }
            }
        },
        "events": {
            "事件:点亮环境": {
                "sequence": [
                    {
                        "filters": {
                            "all_of": [
                                {
                                    "test": "has_tag",
                                    "operator": "!=",
                                    "value": "bullet_hit"
                                }
                            ]
                        },
                        "run_command": {
                            "command": [
                                "fill ~~~ ~~~ light_block 15 keep",
                                "event entity @s 事件:实体消失",
                                "tag @s add bullet_hit"
                            ]
                        }
                    }
                ]
            },
            "事件:弹道粒子": {
                "run_command": {
                    "command": [
                        "particle 动态轨迹:靛蓝 ~~~"
                    ]
                }
            },
            "事件:实体消失": {
                "add": {
                    "component_groups": [
                        "消失"
                    ]
                }
            }
        }
    }
}