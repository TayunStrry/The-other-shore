{
    "format_version": "1.19.10",
    "minecraft:entity": {
        "description": {
            "identifier": "矩阵接口:锚点",
            "is_spawnable": false,
            "is_summonable": true,
            "is_experimental": false
        },
        "component_groups": {
            "销毁.等待.30秒": {
                "minecraft:ageable": {
                    "duration": 30,
                    "grow_up": {
                        "event": "事件:实体消失",
                        "target": "self"
                    }
                }
            },
            "实体消失": {
                "minecraft:instant_despawn": {}
            }
        },
        "components": {
            "minecraft:type_family": {
                "family": [
                    "Matrix_API",
                    "Matrix_Anchor"
                ]
            },
            "minecraft:damage_sensor": {
                "triggers": [
                    {
                        "cause": "*",
                        "deals_damage": false
                    }
                ]
            },
            "minecraft:physics": {
                "has_gravity": false,
                "has_collision": false
            },
            "minecraft:pushable": {
                "is_pushable": false,
                "is_pushable_by_piston": false
            },
            "minecraft:collision_box": {
                "width": 0.1,
                "height": 0.1
            },
            "minecraft:conditional_bandwidth_optimization": {
                "conditional_values": {
                    "max_dropped_ticks": 20,
                    "max_optimized_distance": 200,
                    "use_motion_prediction_hints": true
                },
                "default_values": {
                    "max_dropped_ticks": 10,
                    "max_optimized_distance": 100,
                    "use_motion_prediction_hints": true
                }
            },
            "minecraft:fire_immune": true,
            "minecraft:nameable": {}
        },
        "events": {
            "实体事件": {
                "add": {
                    "component_groups": [
                        "销毁.等待.30秒"
                    ]
                }
            },
            "事件:实体消失": {
                "add": {
                    "component_groups": [
                        "实体消失"
                    ]
                }
            }
        }
    }
}