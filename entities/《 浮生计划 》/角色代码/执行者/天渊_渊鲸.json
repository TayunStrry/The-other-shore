{
    "format_version": "1.19.10",
    "minecraft:entity": {
        "description": {
            "identifier": "天渊阁:渊鲸",
            "is_spawnable": true,
            "is_summonable": true,
            "is_experimental": false,
            "scripts": {
                "animate": [
                    "death"
                ]
            },
            "animations": {
                "death": "controller.animation.data_death"
            }
        },
        "component_groups": {
            "号令": {
                "minecraft:angry": {
                    "duration": 4,
                    "broadcast_anger": true,
                    "broadcast_range": 32,
                    "broadcast_targets": [
                        "abyss"
                    ],
                    "broadcast_filters": {
                        "test": "is_visible",
                        "subject": "other",
                        "value": true
                    },
                    "calm_event": {
                        "event": "事件:放弃进攻",
                        "target": "self"
                    }
                }
            },
            "发射": {
                "minecraft:timer": {
                    "time": 1.0,
                    "looping": true,
                    "time_down_event": {
                        "event": "事件:发起号令",
                        "target": "self"
                    }
                }
            },
            "基础": {
                "minecraft:on_target_acquired": {
                    "event": "事件:发现目标[基础]",
                    "target": "self"
                },
                "minecraft:variant": {
                    "value": 0
                },
                "minecraft:scale": {
                    "value": 3
                },
                "minecraft:tameable": {
                    "probability": 0.15,
                    "tame_items": "minecraft:pumpkin_pie",
                    "tame_event": {
                        "event": "事件:缔结契约",
                        "target": "self"
                    }
                },
                "minecraft:movement": {
                    "value": 0.15
                },
                "minecraft:underwater_movement": {
                    "value": 0.15
                }
            },
            "强化": {
                "minecraft:on_target_acquired": {
                    "event": "事件:发现目标[强化]",
                    "target": "self"
                },
                "minecraft:variant": {
                    "value": 1
                },
                "minecraft:scale": {
                    "value": 2
                },
                "minecraft:movement": {
                    "value": 0.20
                },
                "minecraft:underwater_movement": {
                    "value": 0.20
                }
            },
            "消失": {
                "minecraft:instant_despawn": {}
            }
        },
        "components": {
            "minecraft:behavior.nearest_attackable_target": {
                "priority": 0,
                "entity_types": [
                    {
                        "filters": {
                            "all_of": [
                                {
                                    "test": "is_family",
                                    "subject": "other",
                                    "operator": "!=",
                                    "value": "abyss"
                                }
                            ]
                        },
                        "within_default": 32
                    }
                ],
                "must_reach": true,
                "must_see": true
            },
            "minecraft:behavior.random_swim": {
                "priority": 3,
                "speed_multiplier": 1.0,
                "xz_dist": 16,
                "y_dist": 4,
                "interval": 0
            },
            "minecraft:behavior.swim_wander": {
                "priority": 4,
                "interval": 0.1,
                "look_ahead": 2.0,
                "speed_multiplier": 1.0,
                "wander_time": 5.0
            },
            "minecraft:behavior.swim_idle": {
                "priority": 5,
                "idle_time": 5.0,
                "success_rate": 0.1
            },
            "minecraft:navigation.generic": {
                "can_path_over_water": false,
                "is_amphibious": false,
                "can_breach": false,
                "can_walk": false,
                "can_sink": false,
                "can_swim": true
            },
            "minecraft:experience_reward": {
                "on_death": "query.last_hit_by_player ? 15 : 1"
            },
            "minecraft:damage_sensor": {
                "triggers": [
                    {
                        "cause": "fall",
                        "deals_damage": false
                    },
                    {
                        "cause": "projectile",
                        "deals_damage": true
                    }
                ]
            },
            "minecraft:collision_box": {
                "width": 1.0,
                "height": 1.0
            },
            "minecraft:movement.sway": {
                "sway_amplitude": 0.0
            },
            "minecraft:type_family": {
                "family": [
                    "monster",
                    "abyss",
                    "ship"
                ]
            },
            "minecraft:breathable": {
                "totalSupply": 10,
                "suffocateTime": 0,
                "breathes_air": true,
                "breathes_lava": true,
                "breathes_water": true,
                "breathes_solids": false
            },
            "minecraft:pushable": {
                "is_pushable": true,
                "is_pushable_by_piston": true
            },
            "minecraft:physics": {
                "has_gravity": true
            },
            "minecraft:despawn": {
                "filters": {
                    "all_of": [
                        {
                            "test": "distance_to_nearest_player",
                            "operator": ">",
                            "value": 64
                        }
                    ]
                }
            },
            "minecraft:health": {
                "value": 100
            },
            "minecraft:loot": {
                "table": "loot_tables/无人机.json"
            },
            "minecraft:fire_immune": true
        },
        "events": {
            "minecraft:entity_spawned": {
                "sequence": [
                    {
                        "run_command": {
                            "command": [
                                "execute as @p[tag=!ambient] run function Data/ambient"
                            ]
                        }
                    },
                    {
                        "randomize": [
                            {
                                "weight": 20,
                                "add": {
                                    "component_groups": [
                                        "基础"
                                    ]
                                }
                            },
                            {
                                "weight": 7,
                                "add": {
                                    "component_groups": [
                                        "强化"
                                    ]
                                }
                            },
                            {
                                "weight": 2,
                                "run_command": {
                                    "command": [
                                        "summon 明镜阁:渊鲸 ~~~"
                                    ]
                                },
                                "add": {
                                    "component_groups": [
                                        "强化"
                                    ]
                                }
                            },
                            {
                                "weight": 1,
                                "run_command": {
                                    "command": [
                                        "summon 明镜阁:渊鲸 ~~~"
                                    ]
                                },
                                "add": {
                                    "component_groups": [
                                        "消失"
                                    ]
                                }
                            }
                        ]
                    }
                ]
            },
            "事件:缔结契约": {
                "run_command": {
                    "command": [
                        "summon 明镜阁:渊鲸 ~~~"
                    ]
                },
                "add": {
                    "component_groups": [
                        "消失"
                    ]
                }
            },
            "事件:发现目标[基础]": {
                "sequence": [
                    {
                        "run_command": {
                            "command": [
                                "summon 制导飞弹:逐渊 ^^^ 事件:噬约鱼雷"
                            ]
                        }
                    },
                    {
                        "add": {
                            "component_groups": [
                                "发射"
                            ]
                        }
                    }
                ]
            },
            "事件:发现目标[强化]": {
                "sequence": [
                    {
                        "run_command": {
                            "command": [
                                "summon 制导飞弹:逐渊 ^^^ 事件:噬约鱼雷",
                                "effect @s invisibility 1 0 true"
                            ]
                        }
                    },
                    {
                        "add": {
                            "component_groups": [
                                "发射"
                            ]
                        }
                    }
                ]
            },
            "事件:发起号令": {
                "sequence": [
                    {
                        "add": {
                            "component_groups": [
                                "号令"
                            ]
                        }
                    },
                    {
                        "remove": {
                            "component_groups": [
                                "发射"
                            ]
                        }
                    }
                ]
            },
            "事件:放弃进攻": {
                "remove": {
                    "component_groups": [
                        "号令"
                    ]
                }
            }
        }
    }
}